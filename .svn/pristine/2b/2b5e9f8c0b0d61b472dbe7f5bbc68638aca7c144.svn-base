<table class="table table-striped table-bordered table-hover table-condensed table-sheet" id="table" data-totalrows="{$page.totalrows}" data-totalpage="{$page.totalpage}" data-nowpage="{$page.nowpage}">
    <thead>
        <tr>
            <th width="40" style="text-align:center;">
                <input type="checkbox" name="selectall">
            </th>
            <th>报警时间</th>
            <th>案件来源</th>
            <th>报警人姓名</th>
            <th>单位及联系方式</th>
            <th>事故发生时间</th>
            <th>事故发生地点</th>
        </tr>
    </thead>
    <tbody>
        <volist name="list" id="vo">
            <tr>
                <td class="text-center">
                    <input type="checkbox" name="selectone" value="{$vo.id}">
                </td>
                <td><a href="{:U('detail?case_id='.$case['id'].'&id='.$vo['id'])}" class="js-open js-end-refresh">{$vo.alarm_time|date='Y-m-d H:i',###}</a></td>
                <td>{$vo.case_source_name|default='-'}</td>
                <td>{$vo.alarm_name|default='-'}</td>
                <td>{$vo.contact|default='-'}</td>
                <td>{$vo.accident_time|date='Y-m-d H:i',###}</td>
                <td>{$vo.accident_place|default='-'}</td>
            </tr>
        </volist>
    </tbody>
</table>
