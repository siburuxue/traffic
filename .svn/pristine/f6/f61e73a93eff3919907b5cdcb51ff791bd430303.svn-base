    <div class="panel-body">
        <div class="table-responsive">
            <table class="table table-bordered table-condensed table-form">
                <tr>
                    <th width="15%">所属大队</th>
                    <td width="35%">
                        <select name="condition_department_id" class="form-control input-sm search-auto">
                            <option value="">请选择</option>
                            <volist name="departmentList" id="vo">
                                <option value="{$vo.id}">{$vo.name}</option>
                            </volist>
                        </select>
                    </td>
                    <th width="15%">办案人</th>
                    <td>
                        <input type="text" class="form-control input-sm search-auto" id="condition_user"  name="condition_user" maxlength="255">
                    </td>
                </tr>
                <tr>
                    <th>事故发生时间</th>
                    <td class="form-inline">
                        <table width="100%">
                            <tr>
                                <td>
                                    <input type="text" class="form-control input-sm search-auto form-datetime" id="condition_start_time"  name="condition_start_time" maxlength="255" readonly style="width: 100%">
                                </td>
                                <td width="30" class="text-center">~</td>
                                <td>
                                    <input type="text" class="form-control input-sm search-auto form-datetime" id="condition_end_time" name="condition_end_time" maxlength="255" readonly style="width: 100%">
                                </td>
                            </tr>
                        </table>

                    </td>
                    <th>事故类型</th>
                    <td>
                        <select name="condition_accident_type" class="form-control input-sm search-auto">
                            <option value="">请选择</option>
                            <volist name="accidentType" id="vo">
                                <option value="{$key}">{$vo}</option>
                            </volist>
                        </select>
                    </td>
                </tr>
                <tr>
                    <th>当事人</th>
                    <td>
                        <input type="text" class="form-control input-sm search-auto" id="condition_client"  name="condition_client" maxlength="255">
                    </td>
                    <th>当事人证件号码</th>
                    <td>
                        <input type="text" class="form-control input-sm search-auto" id="condition_idno"  name="condition_idno" maxlength="255">
                    </td>
                </tr>
                <tr>
                    <th>交通方式</th>
                    <td>
                        <select name="condition_traffic_type" class="form-control input-sm search-auto">
                            <option value="">请选择</option>
                            <volist name="traffic_type" id="vo">
                                <option value="{$key}">{$vo}</option>
                            </volist>
                        </select>
                    </td>
                    <th width="15%">车牌号</th>
                    <td>
                        <input type="text" class="form-control input-sm search-auto" id="condition_car_no"  name="condition_car_no" maxlength="255">
                    </td>
                </tr>
            </table>
        </div>
        <button type="button" class="btn btn-primary" id="search-submit"><span class="glyphicon glyphicon-search"></span> 查询</button>
        <button type="button" class="btn btn-warning" id="search-reset"><span class="glyphicon glyphicon-repeat"></span> 重置</button>
    </div>

