<div class="container-fluid" id="approval-box" style="display:none;margin-top:10px;">
<div class="panel-body">
    <table class="table table-bordered table-condensed table-custom">
        <tr>
            <th width="20%">退回缘由</th>
            <td>
                <textarea class="form-control input-sm " name="back_reason" id="back_reason" style="width:100%;height:80px;outline:none;resize:none;" maxlength="65530"></textarea>
            </td>
        </tr>
    </table>
    <button type="button" class="btn btn-primary" id="approval-submit-jimmy"><span class="glyphicon glyphicon-saved"></span> 确定</button>
    <!-- <button type="button" class="btn btn-warning" id="approval-close"><span class="glyphicon glyphicon-repeat"></span> 关闭</button> -->
