<table class="table table-striped table-bordered table-hover table-condensed table-sheet" id="historyTable">
    <thead>
    <tr>
        <th>序号</th>
        <th>送审时间</th>
        <th>审核状态</th>
        <th width="80">操作</th>
    </tr>
    </thead>
    <tbody>
    <volist name="checkList" id="vo">
        <tr>
            <td>{$i}</td>
            <td>{$vo.submit_time|date='Y-m-d H:i',###}</td>
            <td>{$vo.status}</td>
            <td><a href="{:U('CognizanceStopCheckView?case_id='.$case['id'].'&id='.$vo['id'].'&stop_id='.$id.'&type=unCognizance')}" class="btn btn-info btn-sm js-open js-end-refresh">查看</a></td>
        </tr>
    </volist>
    </tbody>
</table>
