<div id="client-list">
    <volist name="clientList" id="vo">
        <div class="panel panel-primary client-item jimmy-click" data-client="{$vo.id}"  data-id="{$vo.id}"  data-name="{$vo.name}" style="cursor:pointer;">
            <!-- 面板标题 -->
            <div class="panel-heading client-list-head" id="{$vo.id}">{$vo.name|default='-'}</div>
            <!-- 面板内容 -->
            <div class="list-group collapse" data-parent="#client-list">
                <div class="list-group-item">交通方式：{$vo.traffic_type}</div>
                <div class="list-group-item">伤害程度：{$vo.hurt_type}</div>
                <div class="list-group-item">车牌：{$vo.car_no}</div>
            </div>
        </div>
    </volist>
</div>
