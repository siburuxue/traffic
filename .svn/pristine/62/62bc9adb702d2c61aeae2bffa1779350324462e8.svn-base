<extend name="Common/base" />
<block name="title"> 案件查询列表</block>
<block name="main.html">
    <!-- 页面主体 -->
    <div class="container-fluid main">
        <!-- 页头 -->
        <div class="head-space"></div>

        <!-- 列表面板 -->
        <div class="panel panel-default">
            <!-- 面板头 -->
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-6"><span class="block-title">案件查询列表</span></div>
                </div>
            </div>
            <!-- 面板主体 -->
            <div class="panel-body">
                <!-- 搜索 -->
                <include file="CaseSearch/index/search" />
                <!-- 表格 -->
                <div id="table-content"></div>
                <!-- 分页 -->
                <include file="Inc/page" />
            </div>
        </div>
        <!-- 表格 结束 -->
    </div>
    <!-- 页面主体 结束 -->
</block>
<block name="foot.html">
</block>
<block name="head.ext">
    <script type="text/javascript">
        // 定义URL地址
        url.table = "{:U('indexTable')}";
    </script>
</block>
<block name="foot.ext">
    <import file="js.CaseSearch.index?" basepath="{$Think.SRC_MODULE_URL}" />
</block>
