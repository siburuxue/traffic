<table class="table table-striped table-bordered table-hover table-condensed table-sheet" id="table" data-totalrows="{$page.totalrows}" data-totalpage="{$page.totalpage}" data-nowpage="{$page.nowpage}">
    <thead>
    <tr>
        <th>姓名</th>
        <th>笔录类型</th>
        <th>次数</th>
        <th>开始时间</th>
        <th>结束时间</th>
        <th>页数</th>
        <th colspan="3">操作</th>
    </tr>
    </thead>
    <tbody>
        <volist name="list" id="vo">
            <tr>
                <td>{$vo.name}</td>
                <td>{$vo.record_type}</td>
                <td>{$vo.record_count}</td>
                <td>{$vo.start_time|date='Y-m-d H:i',###}</td>
                <td>{$vo.end_time|date='Y-m-d H:i',###}</td>
                <td><a href="{:U('photoTable?case_id='.$case['id'].'&id='.$vo['id'].'&type='.$vo['record_type'])}" class="js-open js-end-refresh page-num" data-id="{$vo.id}">{$vo.num}</a></td>
                <td width="120">
                    <a href="javascript:;" class="btn btn-info btn-sm">打印笔录</a>
                </td>
                <td width="60">
                    <a href="{:U('edit?case_id='.$case['id'].'&id='.$vo['id'])}" class="btn btn-info btn-sm js-open">修改</a>
                </td>
                <td width="60">
                    <a href="{:U('delete?case_id='.$case['id'].'&id='.$vo['id'])}" class="btn btn-warning btn-sm js-ajax js-confirm js-end-refresh">删除</a>
                </td>
                <td style="display: none;">{$vo.record_type}</td>
            </tr>
        </volist>
    </tbody>
</table>
