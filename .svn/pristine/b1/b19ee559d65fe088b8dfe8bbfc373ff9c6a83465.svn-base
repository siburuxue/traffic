<table class="table table-striped table-bordered table-hover table-condensed table-sheet" id="table" data-totalrows="{$page.totalrows}" data-totalpage="{$page.totalpage}" data-nowpage="{$page.nowpage}">
    <thead>
        <tr>
            <th style="width:10%;">更改前状态</th>
            <th style="width:10%;">更改后状态</th>
            <th style="width:70%;">更改原因</th>
            <th style="width:10%;">更改时间</th>
        </tr>
    </thead>
    <tbody>
        <volist name="list" id="vo">
            <tr>
                <td>
                    <eq name="vo.pre_status" value="0">未使用</eq>
                    <eq name="vo.pre_status" value="1">已使用</eq>
                </td>
                <td>
                    <eq name="vo.status" value="0">未使用</eq>
                    <eq name="vo.status" value="1">已使用</eq>
                </td>
                <td>
                    {$vo.reason|mb_substr=0,80,"utf-8"}
                </td>
                <td>
                    {$vo.create_time|date="Y-m-d H:i",###}
                </td>
            </tr>
        </volist>
    </tbody>
</table>
