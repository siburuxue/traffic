<div class="panel panel-default">
    <div class="panel-body">
        <div class="table-responsive">
            <table class="table table-bordered table-condensed table-custom">
                <tr>
                    <th>档案编号</th>
                    <td>
                        <input type="text" class="form-control input-sm" name="archive_code" maxlength="50">
                    </td>                 
                </tr>
                <tr>
                    <th>档案名称</th>
                    <td>
                        <input type="text" class="form-control input-sm" name="archive_name" maxlength="250">
                    </td>                 
                </tr>
                <tr>
                    <th>目录号</th>
                    <td>
                        <input type="text" class="form-control input-sm" name="archive_catalog" maxlength="50">
                    </td>                 
                </tr>
                <tr>
                    <th>案卷号</th>
                    <td>
                        <input type="text" class="form-control input-sm" name="archive_dossier" maxlength="50">
                    </td>                 
                </tr>
                <tr>
                    <th>案件号</th>
                    <td>
                        <input type="text" class="form-control input-sm" name="archive_case_no" maxlength="50">
                    </td>                 
                </tr>
                <tr>
                    <th>所在位置</th>
                    <td>
                        <input type="text" class="form-control input-sm" name="archive_place" maxlength="250">
                    </td>
                </tr>
            </table>
        </div>
        <button type="button" class="btn btn-primary" id="archive-submit"><span class="glyphicon glyphicon-search"></span> 保存</button>
        <button type="button" class="btn btn-warning "  id="reset-jimmy-backData"><span class="glyphicon glyphicon-repeat"></span> 重置</button>
    </div>
</div>
