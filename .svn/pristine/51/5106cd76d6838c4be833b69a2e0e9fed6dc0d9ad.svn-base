                        <td   width="30%">
                            <select name="reason_man_pid" class="form-control input-sm post-gather post-gather reason_man_pid"  data-code="1"  data-rank="default_rank">
                                <option value="">请选择</option>
                                <foreach name="case_ext_reason.reason_type.1.value" item="vo">
                                    <option value="{$key}">{$vo.name}</option>
                                </foreach>
                            </select>
                        </td>
                        <td>
                            <select name="reason_man_cid" class="form-control input-sm post-gather post-gather reason_man_cid" data-rank="default_rank">
