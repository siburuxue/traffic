<table class="table table-striped table-bordered table-hover table-condensed table-sheet" id="table" data-totalrows="{$page.totalrows}" data-totalpage="{$page.totalpage}"
	   data-nowpage="{$page.nowpage}">
	<thead>
	<tr>
		<th>编号</th>
		<th>所属大队</th>
		<th>办案人</th>
		<th>被鉴定人（车、物）</th>
		<th>事故时间</th>
		<th>检验鉴定时间</th>
		<th>鉴定项目</th>
		<th>受委托单位</th>
		<th>结论做出时间</th>
	</tr>
	</thead>
	<tbody>
	<volist name="list" id="vo">
		<tr>
			<td>{$vo.id|default='-'}</td>
			<td>{$vo.department_name|default='-'}</td>
			<td>{$vo.handle_true_name|default='-'}</td>
			<td>
				<switch name="vo.checkup_org_item_pid">
					<case value="1">{$vo.target_case_client_name}</case>
					<case value="2">{$vo.target_case_car_no}</case>
					<case value="3">{$vo.target_other}</case>
					<default/>
					-
				</switch>
			</td>
			<td>{$vo.case_accident_time|date='Y-m-d H:i',###}</td>
			<td>{$vo.create_time|date='Y-m-d H:i',###}</td>
			<td>{$vo.checkup_org_item_name|default='-'}</td>
			<td title="{$vo.checkup_org_name}">{$vo.checkup_org_name|default='-'|left_str=###,18}</td>
			<td>
				<notempty name="vo.case_checkup_report_finish_time">
					{$vo.case_checkup_report_finish_time|date='Y-m-d H:i',###}
					<else/>
					-
				</notempty>
			</td>
		</tr>
	</volist>
	</tbody>
</table>
